	wait(0.5)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	wait(60)
	tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)
	 
	tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(game:GetService("Workspace").QuestInfo.DeliverSpawn.Position)})
	tween:Play()
	while wait(3600) do
	tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)
	 
	tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(game:GetService("Workspace").NPCS.Quests.Fred.HumanoidRootPart.Position)})
	tween:Play()
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	wait(60)
	tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)
	 
