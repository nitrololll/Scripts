local function GDRHT_fake_script() -- Dirt.LocalScript 
	local script = Instance.new('LocalScript', Dirt)

	script.Parent.MouseButton1Click:Connect(function()
	for i,v in pairs(game.Workspace.Matches:GetDescendants()) do
	if v:IsA("Seat") and v.Name:match("Player") then
	local oldpos = v.Position
	v.Position = Vector3.new(10000000,10000000,1000000)
	end
	end
	
	while wait(40) do
	for i,v in pairs(game.Workspace.Dirt:GetDescendants()) do
	if v:IsA("ClickDetector") and v.Parent.Name == "Dirt" then
	local egg = v.Parent.Position
	tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)
	
	tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(egg)})
	tween:Play()
	wait(5)
	fireclickdetector(v)
	wait(14)
	fireclickdetector(game:GetService("Workspace").NPCS.Trash.ClickDetector)
	end
	end
	end
	end)
	end
coroutine.wrap(GDRHT_fake_script)()
local function IATFHZ_fake_script() -- Fred.Box 
	local script = Instance.new('LocalScript', Fred)

	script.Parent.MouseButton1Click:Connect(function()
	wait(3)
	fireclickdetector(game:GetService("Workspace").NPCS["Gym Teleport"].ClickDetector)
	wait(1)
	tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)
	 
	tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(game:GetService("Workspace").NPCS.Quests.Fred.HumanoidRootPart.Position)})
	tween:Play()
	wait(3)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.5)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
	fireclickdetector(game:GetService("Workspace").NPCS.Quests.Fred.ClickDetector)
	wait(0.4)
