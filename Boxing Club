-- Scripts:

local function UFILJB_fake_script() -- Rainbow.LocalScript 
	local script = Instance.new('LocalScript', Rainbow)

	            local frame = script.Parent
	            local RunService = game:GetService("RunService")
	            
	            while RunService.RenderStepped:Wait() do
	                for hue = 0,1, 0.01 do
	                    frame.BackgroundColor3 = Color3.fromHSV(hue,1,1)
	                            wait(0.1)
	        end
	    end
	
end
coroutine.wrap(UFILJB_fake_script)()
local function UEEYQO_fake_script() -- Button.LocalScript 
	local script = Instance.new('LocalScript', Button)

	local tog = false
	script.Parent.MouseButton1Click:Connect(function()
		if tog == false then
			tog = true
			script.Parent:TweenPosition(UDim2.new(0, 25,0, 0),"Out","Linear",0.1,false,nil)
			local numb = tonumber(script.Parent.Parent.SpeedAmt.Text)
			        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = numb
		else
			if tog == true then
			tog = false
			script.Parent:TweenPosition(UDim2.new(0, 0,0, 0),"Out","Linear",0.1,false,nil)
		        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
			end
		end
	end)
end
coroutine.wrap(UEEYQO_fake_script)()
local function VWRONMR_fake_script() -- Main.Drag 
	local script = Instance.new('LocalScript', Main)

	script.Parent.Draggable  = true
	script.Parent.Active = true
	script.Parent.Selectable = true
end
coroutine.wrap(VWRONMR_fake_script)()
local function NFROUF_fake_script() -- Squats.LocalScript 
	local script = Instance.new('LocalScript', Squats)

	local tog = false
	script.Parent.MouseButton1Click:Connect(function()
		if tog == false then
			tog = true
	_G.tool = "Squat" -- Sit, Push, Squat
	 
	local plr = game.Players.LocalPlayer
	local bag = plr.Backpack
	local chr = plr.Character
	 
	function activate()
	    for i1, v1 in pairs(chr:GetChildren()) do
	        if v1.Name:match(_G.tool) then
	            v1:Activate()
	        end
	    end
	end
	function parent()
	    for i2, v2 in pairs(bag:GetChildren()) do
	        if v2.Name:match(_G.tool) then
	            v2.Parent = chr
	        end
	    end
	end
	function buy()
	    for i, v in pairs(workspace.Shop:GetChildren()) do
	        if v.Name:match(_G.tool) then
	            fireclickdetector(v:FindFirstChild 'ClickDetector', 0)
	            fireclickdetector(v:FindFirstChild 'ClickDetector', 1)
	        end
	    end
	end
	 
	function sup()
	for i2, v2 in pairs(bag:GetChildren()) do
	    if v2.Name:match(_G.tool) then
	        _G.bagtool = true
	    else
	        _G.bagtool = false
	    end
	end
	for i1, v1 in pairs(chr:GetChildren()) do
	    if v1.Name:match(_G.tool) then
	        _G.chrtool = true
	    else
	        _G.chrtool = false
	    end
	end
	end
	 
	 
	game:service'RunService'.RenderStepped:connect(function()
	    sup()
	end)
	 
	while wait() do
	    if _G.bagtool == false and _G.chrtool == false then
	        buy()
	    elseif _G.bagtool == true then
	        parent()
	    elseif _G.chrtool == true then
	        activate()
	    end
	end
	 
		 end
		end)
end
coroutine.wrap(NFROUF_fake_script)()
